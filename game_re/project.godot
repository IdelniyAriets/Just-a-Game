config_version=5

[application]

config/name="Game_desktop"
config/version="1.2.2"
run/main_scene="res://Scenes/Menu/start_scn.tscn"
config/features=PackedStringArray("4.3", "Mobile")
boot_splash/bg_color=Color(0, 0, 0, 1)

[autoload]

Signals="*res://Scenes/Signals.gd"
GlobalVar="*res://Scenes/global_var.gd"
Rain="*res://Scenes/rain/rain.tscn"
Manager="*res://Scenes/manager/manager.tscn"

[display]

window/size/viewport_width=1366
window/size/viewport_height=768
window/size/mode=3
window/size/borderless=true
window/size/transparent=true
window/size/extend_to_title=true
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor]

run/main_run_args="--rendering-driver opengl3"
script/bg_color=Color(0.14, 0.14, 0.14, 1)

[editor_plugins]

enabled=PackedStringArray("res://addons/AS2P/plugin.cfg")

